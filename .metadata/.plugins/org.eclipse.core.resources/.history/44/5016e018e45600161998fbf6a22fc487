package com.lollibond.chat.data;

public class ThreadParam {

	String fromUser;
	String toUser;
	/*Optional*/
	int pageNumber;
	int pageSize;

	public String getFromUser() {
		return fromUser;
	}

	public void setFromUser(String fromUser) {
		this.fromUser = fromUser;
	}

	public String getToUser() {
		return toUser;
	}

	public void setToUser(String toUser) {
		this.toUser = toUser;
	}

}

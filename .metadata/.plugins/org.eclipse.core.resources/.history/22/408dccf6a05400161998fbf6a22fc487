package com.lollibond.chat.domain;

import java.util.UUID;

import org.springframework.data.cassandra.mapping.PrimaryKey;
import org.springframework.data.cassandra.mapping.Table;

@Table(value = "message_thread_121")
public class MessageThread121 {

	@PrimaryKey
	private UUID messageId;
	@PartitionKey(0)
	private String fromUser;
	private String toUser;
	private String body;
	private UUID time;

	public MessageThread121(UUID message_id, String from_user, String to_user, String body, UUID time) {
		this.messageId = message_id;
		this.fromUser = from_user;
		this.toUser = to_user;
		this.body = body;
		this.time = time;
	}

	public UUID getMessageId() {
		return messageId;
	}

	public void setMessageId(UUID messageId) {
		this.messageId = messageId;
	}

	public String getFromUser() {
		return fromUser;
	}

	public void setFromUser(String fromUser) {
		this.fromUser = fromUser;
	}

	public String getToUser() {
		return toUser;
	}

	public void setToUser(String toUser) {
		this.toUser = toUser;
	}

	public String getBody() {
		return body;
	}

	public void setBody(String body) {
		this.body = body;
	}

	public UUID getTime() {
		return time;
	}

	public void setTime(UUID time) {
		this.time = time;
	}



}
